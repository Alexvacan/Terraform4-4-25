{
	"name": "dw_db_pizzas_type",
	"properties": {
		"folder": {
			"name": "DB_PIZZAS"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "tipo_pizzas_csv",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_dim_pizza_type",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "aggregate1",
					"description": "AgregaciÃ³n de datos por \"\"\"\"pizza_type_id\"\"\" "
				}
			],
			"scriptLines": [
				"source(output(",
				"          {\"\"\"pizza_type_id\"\"} as string,",
				"          {\"\"name\"\"} as string,",
				"          {\"\"category\"\"\";;;;;;} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 select(mapColumn(",
				"          {\"\"\"pizza_type_id\"\"},",
				"          {\"\"name\"\"},",
				"          {\"\"category\"\"\";;;;;;}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 aggregate(groupBy({\"\"\"pizza_type_id\"\"}),",
				"     each(match(name != '\"\"\"pizza_type_id\"\"'), $$ = first($$))) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          pizza_type_id as string,",
				"          name as string,",
				"          category as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          pizza_type_id = {\"\"\"pizza_type_id\"\"},",
				"          name = {\"\"name\"\"},",
				"          category = {\"\"category\"\"\";;;;;;}",
				"     )) ~> sink1"
			]
		}
	}
}